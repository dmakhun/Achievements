<%@ include file="../libs/libs.jsp" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html class="lt-ie9" lang="en">
<html lang="en">
<head>
<link rel="stylesheet" href="css/style.css">
</head>

<body>
	<div class="container">
		<section class="register">
		<center>
			<h1>
				My profile<br>
			</h1>
			<h3>${mess}</h3>
		</center>
		<form method="get">
			<div class="reg_section personal_info">
				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">Change password</div>

					<div class="col-md-4">
						<a href="passwordchanging"> set new password</a>
					</div>
				</div>

				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">First name</div>

					<div class="col-md-4">
						<input class="form-control" type="text" name="name"
							placeholder="First name">
					</div>
				</div>

				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">Last name</div>

					<div class="col-md-4">
						<input class="form-control" type="text" name="surname"
							placeholder="Last name">
					</div>
				</div>

				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">email</div>

					<div class="col-md-4">
						<input class="form-control" type="e-mail" name="email"
							placeholder=e-mail>
					</div>
				</div>

				<div class="row form-group">
					<div class="col-md-offset-6 col-md-4">
						<div class="btn-group btn-group-lg">
							<button class="btn btn-default" type="submit"
								name="removeCompetence">
								<spring:message code="update" />
							</button>
						</div>
					</div>
				</div>
			</div>
		</form>



		</section>
	</div>
	<br>
	<br>
	<section class="about">
	<center>
		© 1993-2014 <a href="http://softserve.ua/">SoftServe</a>
	</center>
	</section>


</body>
</html>