<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<link rel="stylesheet" href="resources/css/addAchiv.css" type="text/css" />
<title>Edit Profile | Achievement board</title>

<link rel="stylesheet" href="css/style.css">
</head>

<body>
	<div class="container">
		<section class="register">
		<center>
			<h1>My profile<br></h1>
			<h3>${mess}</h3>
		</center>
		<form method="get">
			<div class="reg_section personal_info">
				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">
						Change password
					</div>
					
					<div class="col-md-4">
					<a href="passwordchanging"> set new password</a>
					</div>
				</div>
				
				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">
						First name
					</div>
					
					<div class="col-md-4">
					<input class="form-control" type="text" name="name" placeholder="First name">
					</div>
				</div>
				
				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">
						Last name
					</div>
					
					<div class="col-md-4">
					<input class="form-control" type="text" name="surname" placeholder="Last name">
					</div>
				</div>

				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">
						email
					</div>
					
					<div class="col-md-4">
					<input class="form-control" type="e-mail" name="email" placeholder=e-mail>
					</div>
				</div>
				
				<div class="row form-group">
					<div class="col-md-offset-6 col-md-4">
						<div class="btn-group btn-group-lg">
							<button class="btn btn-default" type="submit"
								name="removeCompetence">
								<spring:message code="update" />
								</button>
						</div>
					</div>
				</div>
			</div>
		</form>


			
		</section>
	</div><br><br>
	<section class="about">
	<center>
