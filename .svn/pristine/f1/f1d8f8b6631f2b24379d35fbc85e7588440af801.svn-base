<%@ include file="../libs/libs.jsp"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html class="lt-ie9" lang="en">
<html lang="en">
<head>
<link rel="stylesheet" href="css/style.css">
</head>

<body>
	<div class="container">
		<section class="register">
		<center>
			<h1>
				My profile<br>
			</h1>
			<h3>${mess}</h3>
			
			<img src= "<c:url value="/showImage/${username}"/>" width="240" height="360" />
			
		</center>
		<br>
		<br>
		<br>
		
		<form method="get">
			<div class="reg_section personal_info">
				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">Edit profile</div>

					<div class="col-md-4">
						<a href="editprofile"> edit profile........</a>
					</div>
				</div>
				
				

				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">First name</div>

					<div >
						<h4>
							${name}<br>
						</h4>
					</div>
				</div>

				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">Last name</div>

					<div >
						<h4>
							${surname}<br>
						</h4>
					</div>
				</div>

				<div class="row form-group">
					<div class="col-md-offset-2 col-md-4">email</div>

					<div >
						<h4>
							${email}
							<br>
						</h4>
					</div>
				</div>

				
			</div>
		</form>
	


		</section>
	</div>
	<br>
	<br>
	<section class="about">
	<center>
		© 1993-2014 <a href="http://softserve.ua/">SoftServe</a>
	</center>
	</section>


